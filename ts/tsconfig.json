{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2016",
    "lib": [
      "es5",
      "es6",
      "es2015",
      "es7",
      "es2016",
      "es2017",
      "es2018",
      "esnext.asynciterable",
      "es2015.collection"
    ],
    "allowJs": true,
    "checkJs": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ]
}
